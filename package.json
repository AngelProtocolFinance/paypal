{
  "name": "@better-giving/paypal",
  "version": "1.0.1",
  "description": "TypeScript type definitions generated from PayPal OpenAPI specifications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "download-specs": "node scripts/download-specs.js",
    "generate": "node scripts/generate-types.js",
    "copy-types": "mkdir -p dist && cp generated/*.ts dist/ && for f in dist/*.ts; do mv \"$f\" \"${f%.ts}.d.ts\"; done && cp generated/index.ts dist/index.d.ts",
    "build": "pnpm run download-specs && pnpm run generate && pnpm run copy-types",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist specs generated"
  },
  "keywords": [
    "paypal",
    "typescript",
    "types",
    "openapi",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "openapi-typescript": "^7.4.4",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/paypal-typescript-types.git"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "publishConfig": {
    "access": "public"
  }
}
