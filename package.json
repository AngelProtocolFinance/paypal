{
	"name": "@better-giving/paypal",
	"version": "1.2.0",
	"description": "TypeScript type definitions generated from PayPal OpenAPI specifications",
	"type": "module",
	"packageManager": "pnpm@10.24.0",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && tsx scripts/download-specs.ts && tsx scripts/generate-types.ts && tsc",
		"release": "changeset publish",
		"clean": "rm -rf dist specs src/generated",
		"format": "biome check --write --unsafe",
		"changeset": "changeset",
		"version": "changeset version"
	},
	"keywords": [
		"paypal",
		"typescript",
		"types",
		"openapi",
		"rest-api"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@changesets/changelog-github": "0.5.2",
		"@changesets/cli": "2.29.8",
		"@types/node": "24.10.1",
		"dotenv-cli": "11.0.0",
		"openapi-typescript": "7.4.4",
		"tsx": "4.19.2",
		"typescript": "5.7.2"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AngelProtocolFinance/paypal.git"
	},
	"publishConfig": {
		"access": "public"
	}
}
