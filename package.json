{
  "name": "@better-giving/paypal",
  "version": "1.0.1",
  "description": "TypeScript type definitions generated from PayPal OpenAPI specifications",
  "type": "module",
  "exports": {
    "./catalog-products": {
      "types": "./dist/catalog-products.d.ts"
    },
    "./disputes": {
      "types": "./dist/disputes.d.ts"
    },
    "./invoices": {
      "types": "./dist/invoices.d.ts"
    },
    "./orders": {
      "types": "./dist/orders.d.ts"
    },
    "./partner-referrals": {
      "types": "./dist/partner-referrals.d.ts"
    },
    "./payment-tokens": {
      "types": "./dist/payment-tokens.d.ts"
    },
    "./payments": {
      "types": "./dist/payments.d.ts"
    },
    "./payouts": {
      "types": "./dist/payouts.d.ts"
    },
    "./shipment-tracking": {
      "types": "./dist/shipment-tracking.d.ts"
    },
    "./subscriptions": {
      "types": "./dist/subscriptions.d.ts"
    },
    "./transaction-search": {
      "types": "./dist/transaction-search.d.ts"
    },
    "./web-experience-profiles": {
      "types": "./dist/web-experience-profiles.d.ts"
    },
    "./webhooks": {
      "types": "./dist/webhooks.d.ts"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts"
    }
  },
  "scripts": {
    "download-specs": "node scripts/download-specs.js",
    "generate": "node scripts/generate-types.js",
    "copy-types": "mkdir -p dist && cp generated/*.ts dist/ && for f in dist/*.ts; do [ $(basename \"$f\") != \"index.ts\" ] && mv \"$f\" \"${f%.ts}.d.ts\" || rm \"$f\"; done",
    "build": "pnpm run download-specs && pnpm run generate && pnpm run copy-types && pnpm tsc",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist specs generated"
  },
  "keywords": [
    "paypal",
    "typescript",
    "types",
    "openapi",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "openapi-typescript": "^7.4.4",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist",
    "helpers",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/paypal-typescript-types.git"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "publishConfig": {
    "access": "public"
  }
}
