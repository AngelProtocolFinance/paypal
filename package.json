{
	"name": "@better-giving/paypal",
	"version": "1.0.9",
	"description": "TypeScript type definitions generated from PayPal OpenAPI specifications",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		},
		"./generated": {
			"types": "./dist/src/generated/index.d.ts",
			"import": "./dist/src/generated/index.js"
		}
	},
	"scripts": {
		"build": "bun run clean && bun scripts/download-specs.ts && bun scripts/generate-types.ts && bunx bunup",
		"release": "bun run build && dotenv -- npm publish",
		"clean": "rm -rf dist specs src/generated",
		"format": "biome check --write --unsafe"
	},
	"keywords": [
		"paypal",
		"typescript",
		"types",
		"openapi",
		"rest-api"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@types/node": "^24.10.1",
		"bunup": "^0.15.14",
		"dotenv-cli": "^11.0.0",
		"openapi-typescript": "^7.4.4",
		"typescript": "^5.7.2"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yourusername/paypal-typescript-types.git"
	},
	"publishConfig": {
		"access": "public"
	}
}
